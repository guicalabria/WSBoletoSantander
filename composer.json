{
    "name": "guicalabria/ws-boleto-santander",
    "description": "WS Boletos Santander são classes criadas para facilitar a integração entre aplicativos feitos em PHP e a geração de boletos online no banco Santander.",
    "homepage": "https://github.com/DenysXavier/WSBoletoSantander",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Denys Xavier",
            "email": "equipe@tiexpert.net",
            "homepage": "http://www.tiexpert.net"
        }
    ],
    "support": {
        "issues": "https://github.com/DenysXavier/WSBoletoSantander/issues",
        "source": "https://github.com/DenysXavier/WSBoletoSantander"
    },
    "require": {
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-xmlwriter": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "fzaninotto/faker": "1.6.*",
        "phpunit/phpunit": "5.6.*",
        "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "TIExpert\\WSBoletoSantander\\": "src/"
        }
    }
}
